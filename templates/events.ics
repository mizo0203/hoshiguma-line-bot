BEGIN:VCALENDAR
PRODID:Among Us
VERSION:2.0
METHOD:PUBLISH
CALSCALE:GREGORIAN
X-WR-CALNAME:Among Us
X-WR-CALDESC:Among Us
X-WR-TIMEZONE:Asia/Tokyo

{% for event in events %}
BEGIN:VEVENT
UID:{{ event.uid }}
DESCRIPTION:{{ event.description }}
DTSTART:{{ event.date.start }}
DTEND:{{ event.date.end }}
SUMMARY:{{ event.summary }}
END:VEVENT
{% endfor %}

BEGIN:VTIMEZONE
TZID:Asia/Tokyo
BEGIN:STANDARD
DTSTART:19700101T000000
TZOFFSETFROM:+0900
TZOFFSETTO:+0900
END:STANDARD
END:VTIMEZONE
END:VCALENDAR
